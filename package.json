        "typeorm": "^0.2.38"
    },
    "devDependencies": {
        "@types/jest": "^27.0.2",
        "@types/node": "^16.11.4",
        "concurrently": "^6.3.0",
        "jest": "^27.3.1",
        "nodemon": "^2.0.14",
