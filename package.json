{
    "name": "@nestjs.pro/template-application",
    "private": true,
    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon dist/main.js\" \"tsc -w -p tsconfig.json\" ",
        "release": "rm -rf dist && tsc && npm version patch && npm publish --access=private"
    },
    "dependencies": {
        "@nestjs.pro/common": "^1.0.130",
        "@nestjs/common": "^6.11.6",
        "@nestjs/core": "^6.11.6",
        "@nestjs/swagger": "^4.2.7",
        "@nestjs/typeorm": "^6.3.1",
        "class-validator": "^0.11.0",
        "dotenv": "^8.2.0",
        "typeorm": "^0.2.22"
    },
    "devDependencies": {
        "@types/jest": "^25.1.2",
        "@types/node": "^13.7.1",
        "concurrently": "^5.1.0",
        "jest": "^25.1.0",
        "nodemon": "^2.0.12",
        "ts-jest": "^25.2.0",
        "typescript": "^3.7.5",
        "wait-on": "^4.0.0"
    }
}
